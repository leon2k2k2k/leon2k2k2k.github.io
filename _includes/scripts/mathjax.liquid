{% if site.enable_math %}
  {% unless page.pseudocode %}
    <!-- MathJax -->
    <script type="text/javascript">
      window.MathJax = {
        tex: {
          macros: {
            RR: "{\\bf R}",
            bold: ["{\\bf #1}", 1],
            mbb: ["{\\mathbb{#1}}", 1],
            mcal: ["{\\mathcal{#1}}",1],
            SLZ: "{\\mathrm{SL}_2(\\mathbb{Z})}",
            MpZ: "{\\mathrm{Mp}_2(\\mathbb{Z})}"
          },
          tags: 'ams',
        },
      };
    </script>
    <script
      defer
      type="text/javascript"
      id="MathJax-script"
      src="{{ site.third_party_libraries.mathjax.url.js }}"
      integrity="{{ site.third_party_libraries.mathjax.integrity.js }}"
      crossorigin="anonymous"
    ></script>
    <script
      defer
      src="{{ site.third_party_libraries.polyfill.url.js }}"
      crossorigin="anonymous"
    ></script>
  {% endunless %}
{% endif %}
